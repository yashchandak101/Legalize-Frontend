{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://legalize-backend.onrender.com"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://legalize-backend.onrender.com/api/:path*"
    },
    {
      "source": "/health",
      "destination": "https://legalize-backend.onrender.com/health"
    }
  ]
}
